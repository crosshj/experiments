		"31517222-A9A7-4EAF-B5F6-60751C0BABA3": [
			{
				"name": "quadcopter",
				"source": "window.quadcopter = this.getObjectByName( 'quadcopter' ); window.bellyLight = this.getObjectByName( 'belly light' ); quadcopter.position.y=250; function keydown(event){  console.log(event.keyCode); switch (event.keyCode) { case 37: console.log('move left'); quadcopter.position.x-=10; break; case 38: console.log('move up'); quadcopter.position.z-=10; break; case 39: console.log('move right'); quadcopter.position.x+=10; break; case 40: console.log('move down'); quadcopter.position.z+=10; break;  case 87: console.log('move higher'); quadcopter.position.y+=10; break;   case 83: console.log('move lower'); quadcopter.position.y-=10; break; default: prevent = false; } } function update( event ) {  bellyLight.target.position.x = quadcopter.position.x; bellyLight.target.position.z = quadcopter.position.z; bellyLight.target.updateMatrixWorld();  }"
