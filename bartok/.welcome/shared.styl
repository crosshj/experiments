/* JS template */
::-webkit-scrollbar { width: 5px; }
::-webkit-scrollbar-track { background: transparent; }
::-webkit-scrollbar-thumb { background-color: transparent; }
::-webkit-scrollbar-thumb:hover { background: #2a2a2a; }

body
	margin 3em 1em
	color #bbb
	font-family sans-serif
	background #1d1d1d
	display flex
	flex-direction column
	justify-content center
	align-items start
	max-width 1010px

	&:hover::-webkit-scrollbar-thumb { background-color:#555 }
	/* fix hover in body margins- ugh */
	&::before
		content ''
		position fixed
		top 0
		bottom 0
		left 0
		right 0
		pointer-events none

@media (min-width: 1024px)
	body
		margin 3em auto

pre
	white-space pre-wrap
	width 100%
	box-sizing border-box
	margin 1em auto
	&.info
		background #8881
		padding 1em
	&.error
		background #f772
		color #fcc
		padding 1em
	&[class*="language-"]
		background #262626 !important

div.block
	width 100%
	box-sizing border-box
	margin 1em auto
	&.info
		background #8881
		padding 1em
	&.error
		background #f772
		color #fcc
		padding 1em
	&[class*="language-"]
		background #262626 !important
		
.form-container
	width 100%
	min-height 3em
	background rgba(136,136,136,0.067)
	padding 1em
	box-sizing border-box

	.field-container
		margin-bottom 0.5em

	input
		width 100%
		background #363636
		border 0
		padding 0.25em 0.5em
		outline none
		color #bcb5a5
		margin-top .25em
		box-sizing border-box

button
	color inherit
	border 1px solid
	background transparent
	min-width 15em
	margin-bottom 2em
	padding 10px
	outline none
	&:hover
		background #8888
		color #ccc

