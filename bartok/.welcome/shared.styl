/* templateMessages.js */

.iframe-container
	position relative
	iframe, &:before
		width 100%
		height 100%
	iframe
		border 0
		position relative
	&:before
		z-index 1
		content ''
		pointer-events none
		position absolute
		background-color #0000
		transition all 0.5s
	&.loading:before, &.error:before
		background-color #222
		background-repeat no-repeat
		background-position center center
		background-size 100px
		filter hue-rotate(-14deg) saturate(0.5)
		background-image interwind
	&.error:before
		filter brightness(0.2)
		background-color pink

