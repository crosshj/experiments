{
    "version": "0.4",
    "main": "service.manifest.json",
    "modules": [
    {
        "resources": [
            "./",
            "./index.service.test.html",
            "./index.sw.html",
            "./index.html",
            "./settings.html",
            "./bartok-logo.png",

            "/shared/vendor/localforage.min.js",

            "/shared/fonts/flUhRq6tzZclQEJ-Vdg-IuiaDsNcIhQ8tQ.woff2",
            "/shared/fonts/seti.woff2",
            "/shared/fonts/codicon.ttf",

            "/shared/css/xterm.css",
            "/shared/css/seti-icons.css",
            "/shared/css/material-icons.css",
            "/shared/css/materialize.min.css"
        ]
    },
    "NOTE: resources for example apps",
    {
        "resources": [
            "/shared/vendor/showdown.1.9.1.min.js",
            "/shared/css/github-markdown.min.css"
        ]
    },
    "NOTE: modules",
    {
        "route": "./modules/Panes.mjs",
        "type": "domComponent",
        "source": "modules/PanesNew.mjs"
    }, {
        "route": "./modules/StatusBar.mjs",
        "type": "domComponent",
        "source": "modules/StatusBar.mjs"
    }, {
        "route": "./modules/ActionBar.mjs",
        "type": "domComponent",
        "source": "modules/ActionBar.mjs"
    }, {
        "route": "./modules/TreeView.mjs",
        "type": "domComponent",
        "source": "modules/TreeView.mjs",
        "resources": [
            "/shared/vendor/js-treeview.1.1.5.js"
        ]
    }, {
        "route": "./modules/LoaderBar.mjs",
        "type": "domComponent",
        "source": "modules/LoaderBar.mjs"
    }, {
        "route": "./modules/Terminal.mjs",
        "type": "domComponent",
        "source": "modules/Terminal.mjs",
        "resources": [
            "/shared/vendor/xterm.min.js",
            "/shared/vendor/xterm-addon-fit.js"
        ]
    }, {
        "route": "./modules/Hotkeys.mjs",
        "type": "domComponent",
        "source": "modules/HotKeys.mjs"
    }, {
        "route": "./modules/Services.mjs",
        "type": "domComponent",
        "source": "modules/Services.mjs"
    }, {
        "route": "./modules/Editor.mjs",
        "type": "domComponent",
        "source": "modules/Editor.mjs",
        "resources": [
            "/shared/modules/editor.mjs",
            "/shared/css/codemirror.css",
            "/shared/css/bespin.css",
            "/shared/css/vscode.codemirror.css",

            "/shared/vendor/codemirror.js",
            "/shared/vendor/codemirror/addons/_fold.bundle.js",
            "/shared/vendor/codemirror-scrollpastend.js",
            "/shared/vendor/codemirror-show-invisibles.js",
            "/shared/vendor/codemirror-search.js",

            "/shared/vendor/codemirror/mode/javascript.js",
            "/shared/vendor/codemirror/mode/jsx.js",
            "/shared/vendor/codemirror/mode/xml.js",
            "/shared/vendor/codemirror/mode/sql.js",
            "/shared/vendor/codemirror/mode/commonlisp.js",
            "/shared/vendor/codemirror/mode/css.js",
            "/shared/vendor/codemirror/mode/clike.js",
            "/shared/vendor/codemirror/mode/htmlmixed.js",
            "/shared/vendor/codemirror/mode/htmlembedded.js",
            "/shared/vendor/codemirror/mode/markdown.js",

            "/shared/vendor/codemirror/mode/perl.js",
            "/shared/vendor/codemirror/mode/python.js",
            "/shared/vendor/codemirror/mode/php.js",
            "/shared/vendor/codemirror/mode/go.js",
            "/shared/vendor/codemirror/mode/clojure.js",
            "/shared/vendor/codemirror/mode/ruby.js"
        ]
    }, {
        "route": "./modules/ContextMenu.mjs",
        "type": "domComponent",
        "source": "modules/ContextMenu.mjs"
    }, {
        "route": "./modules/Operations.mjs",
        "type": "domComponent",
        "source": "modules/Operations_New.mjs"
    },

    "NOTE: should be a handlers in service worker context",
    {
        "route": "./modules/events/statusBar.mjs",
        "source": "modules/events/statusBar.mjs",
        "handler": null
    }, {
        "route": "./modules/events/tree.mjs",
        "source": "modules/events/tree.mjs",
        "handler": null
    }, {
        "route": "./modules/events/loader.mjs",
        "source": "modules/events/loader.mjs",
        "handler": null
    }, {
        "route": "./modules/events/terminal.mjs",
        "source": "modules/events/terminal.mjs",
        "handler": null
    }, {
        "route": "./modules/events/services.mjs",
        "source": "modules/events/services.mjs",
        "handler": null
    }, {
        "route": "./modules/events/templates.mjs",
        "source": "modules/events/templates.mjs",
        "handler": null
    }, {
        "route": "./modules/events/editor.mjs",
        "source": "modules/events/editor.mjs",
        "handler": null
    }, {
        "route": "./modules/events/editorTabs.mjs",
        "source": "modules/events/editorTabs.mjs",
        "handler": null
    }, {
        "route": "./modules/events/operations.mjs",
        "source": "modules/events/operations.mjs",
        "handler": null
    },

    "NOTE: should be compiled in to each module",
    {
        "route": "./modules/Listeners.mjs",
        "source": "modules/Listeners.mjs",
        "handler": null
    }, {
        "route": "./modules/Templates.mjs",
        "source": "modules/Templates.mjs",
        "handler": null
    }, {
        "route": "./modules/EditorTabs.mjs",
        "source": "modules/EditorTabs.mjs"
    }, {
        "route": "./modules/motd.mjs",
        "source": "modules/motd.mjs",
        "handler": null
    }, {
        "route": "./modules/state.mjs",
        "source": "modules/state.mjs",
        "handler": null
    }, {
        "route": "./modules/ExternalState.mjs",
        "source": "modules/ExternalState.mjs",
        "handler": null
    }, {
        "route": "./modules/operationsManagement.mjs",
        "source": "modules/operationsManagement.mjs",
        "handler": null
    }, {
        "route": "./modules/operationsService.mjs",
        "source": "modules/operationsService.mjs",
        "handler": null
    }, {
        "route": "./modules/Types.mjs",
        "source": "modules/Types.mjs",
        "handler": null
    }, {
        "route": "/shared/modules/utilities.mjs",
        "source": "/shared/modules/utilities.mjs",
        "handler": null
    }, {
        "route": "/shared/icons/seti/ext.json.mjs",
        "source": "/shared/icons/seti/ext.json.mjs",
        "handler": null
    },

    "NOTE: should be bundled and minified?",
    {
        "route": "./index.css",
        "source": "index.css",
        "include": [
            "/shared/css/xterm.css",
            "/shared/css/seti-icons.css",
            "/shared/css/material-icons.css",
            "/shared/css/materialize.min.css"
        ]
    },

    "NOTE: external handlers",
    {
        "route": "\/service\/(.*)",
        "type": "fetch",
        "handler": "./modules/serviceRequestHandler.js"
    },
    {
        "route": "\/manage\/(.*)",
        "type": "fetch",
        "handler": "./modules/serviceRequestHandler.js"
    },
    {
        "route": "\/monitor\/(.*)",
        "type": "fetch",
        "handler": "./modules/serviceRequestHandler.js"
    },
    {
        "route": "\/persist\/(.*)",
        "type": "fetch",
        "handler": "./modules/serviceRequestHandler.js"
    },

    "NOTE: this should not be defined here, instead it should be dynamically loaded",
    {
        "route": "\/.welcome\/(.*)",
        "type": "fetch",
        "handler": "./modules/serviceRequestHandler.js"
    },

    "NOTE: end modules"]
}